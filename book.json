{
    "author": "larryhou",
    "title": "abtool",
    "description": "Unity开发的梦中情人❬abtool❭",
    "plugins": [
        "mathjax",
        "summary-tree-number",
        "splitter",
        "popup",
        "code",
        "chapter-fold",
        "-lunr",
        "-search",
        "search-pro",
        "back-to-top-button"
    ],
    "pluginsConfig": {},
    "pdf": {
        "pageNumbers": true,
        "fontFamily": "Arial",
        "fontSize": 12,
        "paperSize": "a4",
        "margin": {
            "right": 0,
            "left": 0,
            "top": 0,
            "bottom": 0
        }
    }
}
